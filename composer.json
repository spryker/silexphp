{
    "name": "spryker/silexphp",
    "description": "The PHP micro-framework based on the Symfony Components",
    "keywords": ["microframework"],
    "homepage": "http://silex.sensiolabs.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "php": ">=7.1",
        "spryker/container": "dev-beta/te-323/te-648-enabling-step-3",
        "symfony/event-dispatcher": "^3.0.0 || ^4.0.0",
        "symfony/http-foundation": "^3.0.0",
        "symfony/http-kernel": "^3.0.0 || ^4.0.0",
        "symfony/routing": "^3.0.0 || ^4.0.0"
    },
    "require-dev": {
        "symfony/security": "~2.3|3.0.*",
        "symfony/config": "~2.3|3.0.*",
        "symfony/intl": "~2.3|3.0.*",
        "symfony/form": "^3.0.0 || ^4.0.0",
        "symfony/browser-kit": "~2.3|3.0.*",
        "symfony/css-selector": "~2.3|3.0.*",
        "symfony/debug": "~2.3|3.0.*",
        "symfony/dom-crawler": "~2.3|3.0.*",
        "symfony/finder": "~2.3|3.0.*",
        "symfony/monolog-bridge": "~2.3|3.0.*",
        "symfony/options-resolver": "~2.3|3.0.*",
        "symfony/phpunit-bridge": "~2.7",
        "symfony/process": "~2.3|3.0.*",
        "symfony/serializer": "~2.3|3.0.*",
        "symfony/translation": "~2.3|3.0.*",
        "symfony/twig-bridge": "~2.3|3.0.*",
        "symfony/validator": "^3.0.0",
        "twig/twig": "~1.28|~2.0",
        "doctrine/dbal": "~2.2",
        "swiftmailer/swiftmailer": "~5",
        "monolog/monolog": "^1.4.1",
        "phpunit/phpunit": "^5 || ^6 || ^7"
    },
    "autoload": {
        "psr-4": { "Silex\\": "src/Silex" }
    },
    "autoload-dev" : {
        "psr-4": { "Silex\\Tests\\" : "tests/Silex/Tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
