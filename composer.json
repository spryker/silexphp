{
    "name": "spryker/silexphp",
    "description": "The PHP micro-framework based on the Symfony Components",
    "keywords": ["microframework"],
    "homepage": "http://silex.sensiolabs.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "php": ">=7.2",
        "spryker/container": "dev-beta/te-7001/te-7174/te-7175-create-beta-branch-for-spryker-container as 1.1.0",
        "symfony/event-dispatcher": "^4.4.0 || ^5.0.0",
        "symfony/http-kernel": "^4.4.0 || ^5.0.0",
        "symfony/routing": "^4.4.0 || ^5.0.0"
    },
    "require-dev": {
        "symfony/security-core": "^4.2.7 < 4.4.0 || ^4.4.7 || ^5.0.0",
        "symfony/security-csrf": "^4.2.7 < 4.4.0 || ^4.4.7 || ^5.0.0",
        "symfony/security-guard": "^4.2.7 < 4.4.0 || ^4.4.7 || ^5.0.0",
        "symfony/security-http": "^4.2.7 < 4.4.0 || ^4.4.7 || ^5.0.0",
        "symfony/config": "^4.4.0 || ^5.0.0",
        "symfony/intl": "^4.4.0 || ^5.0.0",
        "symfony/form": "^4.4.0 || ^5.0.0",
        "symfony/browser-kit": "^4.4.0 || ^5.0.0",
        "symfony/css-selector": "^4.4.0 || ^5.0.0",
        "symfony/debug": "^4.4.0 || ^5.0.0",
        "symfony/dom-crawler": "^4.4.0 || ^5.0.0",
        "symfony/finder": "^4.4.0 || ^5.0.0",
        "symfony/monolog-bridge": "^4.4.0 || ^5.0.0",
        "symfony/options-resolver": "^4.4.0 || ^5.0.0",
        "symfony/process": "^4.4.0 || ^5.0.0",
        "symfony/serializer": "^4.4.0 || ^5.0.0",
        "symfony/translation": "^4.4.0 || ^5.0.0",
        "symfony/twig-bridge": "^4.4.0 || ^5.0.0",
        "symfony/validator": "^4.4.0 || ^5.0.0",
        "twig/twig": "^1.28.0|^2.0.0|^3.0.0",
        "doctrine/dbal": "~2.2",
        "swiftmailer/swiftmailer": "^5.0.0 || ^6.0.0",
        "monolog/monolog": "^1.4.1 || ^2.0.0",
        "phpunit/phpunit": "^8.0.0 || ^9.0.0",
        "sllh/composer-versions-check": "^2.0"
    },
    "conflict": {
        "silexphp/silex": "<3.0.0"
    },
    "autoload": {
        "psr-4": { "Silex\\": "src/Silex" }
    },
    "autoload-dev" : {
        "psr-4": { "Silex\\Tests\\" : "tests/Silex/Tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    },
    "scripts": {
      "phpstan": "phpstan analyse -c phpstan.neon -l 5 src/",
      "phpstan-setup": "cp composer.json composer.backup && COMPOSER_MEMORY_LIMIT=-1 composer require --dev phpstan/phpstan-shim:^0.11 && mv composer.backup composer.json"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
