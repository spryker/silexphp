{
    "name": "spryker/silexphp",
    "description": "The PHP micro-framework based on the Symfony Components",
    "keywords": [
        "microframework"
    ],
    "homepage": "http://silex.sensiolabs.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Igor Wiedler",
            "email": "igor@wiedler.ch"
        }
    ],
    "require": {
        "php": ">=8.0",
        "spryker/container": "^1.1.0",
        "symfony/event-dispatcher": "^5.0.0 || ^6.0.0",
        "symfony/http-kernel": "^5.0.0 || ^6.0.0",
        "symfony/routing": "^5.0.0 || ^6.0.0"
    },
    "require-dev": {
        "symfony/security-core": "^5.0.0 || ^6.0.0",
        "symfony/security-csrf": "^5.0.0 || ^6.0.0",
        "symfony/security-guard": "^5.0.0 || ^6.0.0",
        "symfony/security-http": "^5.0.0 || ^6.0.0",
        "symfony/config": "^5.0.0 || ^6.0.0",
        "symfony/intl": "^5.0.0 || ^6.0.0",
        "symfony/form": "^5.0.0 || ^6.0.0",
        "symfony/browser-kit": "^5.0.0 || ^6.0.0",
        "symfony/css-selector": "^5.0.0  || ^6.0.0",
        "symfony/debug": "^3.0.0 || ^4.0.0",
        "symfony/dom-crawler": "^5.0.0 || ^6.0.0",
        "symfony/finder": "^5.0.0 || ^6.0.0",
        "symfony/monolog-bridge": "^5.0.0 || ^6.0.0",
        "symfony/options-resolver": "^5.0.0 || ^6.0.0",
        "symfony/process": "^5.0.0 || ^6.0.0",
        "symfony/serializer": "^5.0.0  || ^6.0.0",
        "symfony/translation": "^5.0.0 || ^6.0.0",
        "symfony/twig-bridge": "^5.0.0 || ^6.0.0",
        "symfony/validator": "^5.0.0 || ^6.0.0",
        "twig/twig": "^1.41 || ^2.0.0 || ^3.0.0",
        "doctrine/dbal": "~2.2",
        "swiftmailer/swiftmailer": "^5.0.0 || ^6.0.0",
        "monolog/monolog": "^1.4.1 || ^2.0.0",
        "phpunit/phpunit": "^9.5.2",
        "sllh/composer-versions-check": "^2.0",
        "phpstan/phpstan": "^1.8.10"
    },
    "conflict": {
        "silexphp/silex": "<3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Silex\\": "src/Silex"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silex\\Tests\\": "tests/Silex/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    },
    "scripts": {
        "phpstan": "phpstan analyse -c phpstan.neon -l 5 src/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "sllh/composer-versions-check": true
        }
    }
}
